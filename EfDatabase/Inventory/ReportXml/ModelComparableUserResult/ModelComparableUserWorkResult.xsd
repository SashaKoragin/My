					<xs:documentation>Описание</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="OperationSystem" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Операционная система</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CpuType" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Тип процессора</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CpuMHz" type="xs:int" use="optional" default="0">
				<xs:annotation>
					<xs:documentation>Частота процессора, МГц</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CoreCount" type="xs:int" use="optional" default="0">
				<xs:annotation>
					<xs:documentation>Количество ядер</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CpuCount" type="xs:int" use="optional" default="0">
				<xs:annotation>
					<xs:documentation>Количество процессоров</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="HddType" type="xs:string">
				<xs:annotation>
					<xs:documentation>Тип жесткого диска</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="HddCount" type="xs:int" use="optional" default="0">
				<xs:annotation>
					<xs:documentation>Количество HDD</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="HddSize" type="xs:int" use="optional" default="0">
				<xs:annotation>
					<xs:documentation>Объем жесткого диска</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="NetworkAdapter" type="xs:string">
				<xs:annotation>
					<xs:documentation>Сетевая карта</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="NetworkAdapterSpeed" type="xs:int" use="optional" default="0">
				<xs:annotation>
					<xs:documentation>Скорость сетевой карты</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Mac" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>МАК Адресс</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="RamType" type="xs:string">
				<xs:annotation>
					<xs:documentation>ОЗУ (тип)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="MemorySize" type="xs:int" use="optional" default="0">
				<xs:annotation>
					<xs:documentation>ОЗУ (размер)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="DiskSize" type="xs:int" use="optional" default="0">
				<xs:annotation>
					<xs:documentation>Общее дисковое пространство, Gb</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CdOrDvd" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>CD/DVD</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CountGroup" type="xs:int" use="required">
				<xs:annotation>
					<xs:documentation>Колличество в группе</xs:documentation>
				</xs:annotation>
			</xs:attribute>
