	<xs:element name="ComparableCardAksiokAndInventory">
		<xs:annotation>
			<xs:documentation>Сравнительный анализ карточки оборудования АКСИОК и Инвентаризации</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Memo" type="xs:string">
				<xs:annotation>
					<xs:documentation>Наименование поля АКСИОК</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Inventory" type="xs:string">
				<xs:annotation>
					<xs:documentation>Данные инвентаризации</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Aksiok" type="xs:string">
				<xs:annotation>
					<xs:documentation>Данные АКСИОК</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
</xs:schema>
