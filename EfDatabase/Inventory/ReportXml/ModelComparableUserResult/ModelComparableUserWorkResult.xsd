<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="ModelComparableUserResultWork" type="ModelComparableUserResultWork">
		<xs:annotation>
			<xs:documentation>Все результаты анализа работы процесса сравнеия данных</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="ModelComparableUserResultWork">
		<xs:annotation>
			<xs:documentation>Все результаты анализа работы процесса сравнеия данных</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="ComparableUserResult" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="ModelComparableAllSystemInventory" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ComparableUserResult" nillable="true">
		<xs:annotation>
			<xs:documentation>Результат работы сравнения пользователей из всех систем</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="LogicsButton" type="xs:string" use="optional" default="Button">
				<xs:annotation>
					<xs:documentation>Логика кнопок</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="NameSystem" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Наименование системы данных</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="IdUser" type="xs:int" use="required">
				<xs:annotation>
					<xs:documentation>Ун пользователя</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FullName" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Полное имя пользователя</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PersonnelNumber" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Табельный номер</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Department" type="xs:string" use="required">
