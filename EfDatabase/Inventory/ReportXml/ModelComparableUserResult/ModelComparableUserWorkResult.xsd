				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="NamePosition" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Должность</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Ranks" type="xs:string" use="required">
